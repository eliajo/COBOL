<?xml version="1.0" encoding="utf-8"?>
<!--
View for Navigation Graph
@author Pegah Amanzadeh
-->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ControlView.Question4">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="265dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="48dp"
        android:src="@drawable/sun"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:visibility="visible" />

    <Button
        android:id="@+id/saveButton"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="118dp"
        android:layout_height="63dp"
        android:layout_marginBottom="14dp"
        android:width="1dip"
        android:radius="15dip"
        android:text="Spara"
        app:backgroundTint="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="353dp"
        android:layout_height="24dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:text="13.Har du druckit alkohol? "
        android:textColor="#176693"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="384dp"
        android:layout_height="37dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="192dp"
        android:layout_marginBottom="239dp"

        android:text="14.Har du haft tvångstankar? "
        android:textColor="#176693"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/chipGroup10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="359dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="280dp"
        android:text="16.Uppskatta din mående *"
        android:textColor="#176693"

        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="366dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="144dp"
        android:text="17.Har det hänt allvarig händelser? beskriv"
        android:textColor="#176693"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="248dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:text="Sida 4 av 4"
        android:textColor="#176693"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText2"
        android:layout_width="372dp"
        android:layout_height="41dp"
        android:layout_marginBottom="88dp"
        android:hint="Beskriv här..."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroup7"
        android:layout_width="132dp"
        android:layout_height="67dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.281"
        app:singleLine="true"
        app:singleSelection="true">

        <com.google.android.material.chip.Chip
            android:id="@+id/chip6"
            style="@style/YesOrNowChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:text="Ja"
            android:textColor="#2D9CDB" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip7"
            style="@style/YesOrNowChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:text="Nej"
            android:textColor="#2D9CDB" />
    </com.google.android.material.chip.ChipGroup>

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroup8"
        android:layout_width="131dp"
        android:layout_height="71dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"

        android:layout_marginTop="108dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:singleLine="true"
        app:singleSelection="true">

        <com.google.android.material.chip.Chip
            android:id="@+id/chip8"
            style="@style/YesOrNowChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:text="Ja"
            android:textColor="#2D9CDB" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip24"
            style="@style/YesOrNowChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:text="Nej"
            android:textColor="#2D9CDB" />
    </com.google.android.material.chip.ChipGroup>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="375dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:text="15.Har gjort någon fysisk aktivitet? "
        android:textColor="#176693"


        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.449"
        app:singleLine="true"
        app:singleSelection="true" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="372dp"
        android:layout_height="41dp"
        android:layout_marginBottom="312dp"
        android:hint="Beskriv här..."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroup10"
        style="@style/OneToTenChipStyle"
        android:layout_width="287dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="178dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.chip.Chip
            android:id="@+id/chip26"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip27"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip33"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip51"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip52"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip53"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip54"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip55"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip56"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"
            app:singleSelection="true"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/chip57"
            style="@style/OneToTenChipStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10"
            app:singleSelection="true"/>
    </com.google.android.material.chip.ChipGroup>


</androidx.constraintlayout.widget.ConstraintLayout>